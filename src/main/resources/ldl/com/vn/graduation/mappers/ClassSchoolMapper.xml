<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ldl.com.vn.graduation.mappers.ClassSchoolMapper">
  <select id="selectAll" resultType="ldl.com.vn.graduation.beans.ClassSchool">
    SELECT
        classS_dl.idC
        , classS_dl.classN 
    FROM
        classS_dl 
    ORDER BY
        idC
  </select>
</mapper>